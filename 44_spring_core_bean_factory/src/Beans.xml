<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="message" class="com.izdebski.model.Message">
        <property name="messageId" value="1001"></property>
        <property name="message" value="HMS Queen Elizabeth- UK"></property>
    </bean>

    <alias name="message" alias="MyMessage1"/>
    <alias name="message" alias="MyMessage2"/>
</beans>